<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:drawable/screen_background_dark_transparent"
    tools:context=".activity_tickets">

    <ImageButton
        android:id="@+id/ibtnprueba"
        android:layout_width="391dp"
        android:layout_height="149dp"
        android:layout_marginTop="112dp"
        android:adjustViewBounds="false"
        android:background="@drawable/prueba1"
        android:scaleType="centerCrop"
        android:src="@drawable/tecnico2"
        android:text="Porque no sale el texto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/ibtnTicket"
        android:layout_width="391dp"
        android:layout_height="149dp"
        android:layout_marginTop="288dp"
        android:adjustViewBounds="false"
        android:background="@drawable/prueba1"
        android:scaleType="centerCrop"
        android:src="@drawable/ticket"
        android:text="Porque no sale el texto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01" />

    <ImageButton
        android:id="@+id/ibtnRefacciones"
        android:layout_width="391dp"
        android:layout_height="149dp"
        android:layout_marginTop="464dp"
        android:adjustViewBounds="false"
        android:background="@drawable/prueba1"
        android:scaleType="centerCrop"
        android:src="@drawable/refacciones"
        android:text="Porque no sale el texto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.101" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="196dp"
        android:layout_height="46dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Refacciones"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.948"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.395" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="122dp"
        android:layout_height="45dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Tickets"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.944"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.724" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="145dp"
        android:layout_height="44dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Tecnicos"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.962"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.131" />


</androidx.constraintlayout.widget.ConstraintLayout>